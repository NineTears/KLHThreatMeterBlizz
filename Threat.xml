				<Anchors>
					<Anchor point="TOP">
						<Offset x="0" y="-3"/>
					</Anchor>
				</Anchors>
			</Texture>
		</Layer>
		<Layer level="ARTWORK">
			<Texture file="Interface\AddOns\KLHThreatMeterBlizz\textures\NumericThreatBorder">
				<TexCoords left="0" right="0.765625" top="0" bottom="0.5625"/>
